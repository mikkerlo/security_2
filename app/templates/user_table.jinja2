{% if users %}
    <table class="table" style="overflow: visible;">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Id</th>
                <th scope="col">User</th>
            </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <th scope="row"> {{ user.id }} </th>
                <td> {{ user.login }} </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endif %}
